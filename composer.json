{
    "name": "faisuc/sportsdataio-php-sdk",
    "description": "This is my package sportsdataio-php-sdk",
    "keywords": [
        "faisuc",
        "sportsdataio-php-sdk"
    ],
    "homepage": "https://github.com/faisuc/sportsdataio-php-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Carlo Sucuangco",
            "email": "necafasu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Faisuc\\SportsdataioPhpSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faisuc\\SportsdataioPhpSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
